{
    "name": "overseegroup/panacea",
    "description": "Laravel Notification Driver for Panacea Mobile.",
    "keywords": ["laravel", "notifications", "smscentre", "smsc"],
    "homepage": "https://github.com/overseegroup/panacea",
    "license": "MIT",
    "authors": [
        {
            "name": "Stan Kobrin",
            "role": "Developer",
            "email": "stan@oversee.group"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/notifications": "^5.3@dev",
        "illuminate/queue": "^5.3@dev",
        "illuminate/support": "^5.3@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Panacea\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Panacea\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev"
}
